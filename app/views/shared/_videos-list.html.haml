.video__inner
  .video__inner__thumnail
    = link_to video_path(video), class: "video__inner__thumnail__link" do
      = image_tag "http://img.youtube.com/vi/#{video.videoID}/mqdefault.jpg"
  .video__inner__details
    %p.video__inner__details__song-title
      = video.title
    %p.video__inner__details__artist-name
      = video.artist.name
    %p.video__inner__details__date
      = video.created_at.strftime("%Y/%m/%d")